{
  "name": "nekochat",
  "version": "2.4.0",
  "description": "The online chat system, part of NekoRPG.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukatama/nekochat.git"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "npm run clean",
    "prestart": "node scripts/prestart.js",
    "start": "node .",
    "build": "run-p -l build:*",
    "build:babel": "babel -s true -d lib --ignore src/pegjs/*.js src",
    "build:fluorite5": "pegjs --cache --allowed-start-rules Expression,VMFactory fluorite5/fluorite5.pegjs lib/pegjs/fluorite5.js",
    "build:webpack": "webpack",
    "clean": "run-s -l clean:rimraf clean:mkdirp",
    "clean:rimraf": "rimraf coverage/* dist/js/* lib/*",
    "clean:mkdirp": "mkdirp dist/js lib lib/pegjs",
    "lint": "run-p -l lint:*",
    "lint:modules": "eslint --color -c ./eslint/modules.yml --ignore-pattern __tests__ --ignore-pattern __mocks__ src **/*.babel.js",
    "lint:scripts": "eslint --color -c ./eslint/scripts.yml --ignore-pattern src --ignore-pattern **/*.babel.js .",
    "lint:jest": "eslint --color -c ./eslint/jest.yml src/**/__tests__/**/*.js src/**/__mocks__/**/*.js",
    "test": "run-p -l test:*",
    "test:jest": "jest --colors",
    "watch": "run-p -l watch:*:* watch:server",
    "watch:build": "run-p -l watch:build:*",
    "watch:build:babel": "npm run -s build:babel -- -w",
    "watch:build:fluorite5": "nodemon -d 1 -w fluorite5 -e pegjs -x \"npm run build:fluorite5\"",
    "watch:build:webpack": "webpack -w",
    "watch:lint": "run-p-l watch:lint:*",
    "watch:lint:modules": "nodemon -d 1 -w src -x \"npm run -s lint:modules\"",
    "watch:lint:scripts": "nodemon -d 1 -i src -i lib -i dist -x \"npm run -s lint:scripts\"",
    "watch:lint:jest": "nodemon -d 1 -w src -x \"npm run -s lint:jest\"",
    "watch:test": "run-p -l watch:test:*",
    "watch:test:jest": "nodemon -d 3 -w src --exec \"npm run -s test:jest -- -i\"",
    "watch:server": "nodemon -d 1 -w lib index.js",
    "production": "run-s -l clean lint:* test:* build:* prod:*",
    "prod:webpack": "cross-env NODE_ENV=production webpack",
    "version": "update-changelog && git add CHANGELOG.md"
  },
  "author": "ukatama",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.13.1",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "color-space": "^1.13.0",
    "config": "^1.19.0",
    "connect-livereload": "^0.5.4",
    "connect-redis": "^3.0.2",
    "connect-session-knex": "^1.0.17",
    "crypto-browserify": "^3.11.0",
    "dice3d": "^1.0.2",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "express-socket.io-session": "^1.3.1",
    "fbemitter": "^2.0.1",
    "format-si-prefix": "^0.3.2",
    "fs-promise": "^0.5.0",
    "immutable": "^3.8.1",
    "is-mobile": "^0.2.2",
    "jade": "^1.11.0",
    "knex": "^0.11.4",
    "lodash": "^4.5.1",
    "log4js": "^0.6.29",
    "material-ui": "^0.15.0",
    "moment": ">=2.11.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pegjs": "^0.9.0",
    "react": "^15.1.0",
    "react-addons-perf": "^15.1.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-immutable-proptypes": "^2.0.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.6.0",
    "react-tap-event-plugin": "^1.0.0",
    "redis": "^2.5.1",
    "redux": "^3.0.5",
    "redux-actions": "^0.10.0",
    "redux-promise": "^0.5.3",
    "redux-router": "^2.0.0",
    "sanitize.css": "^4.1.0",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.4.4",
    "source-map-support": "^0.4.0",
    "update-changelog": "^1.0.1",
    "why-did-you-update": "0.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-jest": "^14.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "cross-env": "^2.0.0",
    "eslint": "^3.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "exorcist": "^0.4.0",
    "jasmine-immutablejs-matchers": "^0.1.2",
    "jest-cli": "^14.0.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.1.1",
    "rimraf": "^2.5.3",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1"
  },
  "jest": {
    "testPathDirs": [
      "src"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov",
      "clover"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jest-setup.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/ansi-regex",
      "<rootDir>/node_modules/any-promise",
      "<rootDir>/node_modules/bluebird",
      "<rootDir>/node_modules/body-parser",
      "<rootDir>/node_modules/bookshelf/node_modules/bluebird",
      "<rootDir>/node_modules/bookshelf/node_modules/lodash",
      "<rootDir>/node_modules/depd",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/knex/node_modules/bluebird",
      "<rootDir>/node_modules/knex/node_modules/lodash",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/recompose",
      "<rootDir>/node_modules/sshpk"
    ]
  }
}
