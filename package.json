{
  "name": "nekochat",
  "version": "2.7.1",
  "description": "The online chat system, part of NekoRPG.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukatama/nekochat.git"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node .",
    "build": "run-p -l build:*",
    "build:babel": "babel -s true -d lib --ignore src/pegjs/*.js --ignore src/**/*.test.js --ignore src/**/__mocks__/**/*.js src",
    "build:fluorite5": "run-s build:fluorite5:*",
    "build:fluorite5:mkdir": "make-dir lib/pegjs",
    "build:fluorite5:pegjs": "pegjs --cache --allowed-start-rules Expression,VMFactory -o lib/pegjs/fluorite5.js fluorite5/fluorite5.pegjs",
    "build:webpack": "webpack",
    "clean": "run-s -l clean:rimraf clean:mkdirp",
    "clean:rimraf": "rimraf coverage/* dist/js/* lib/*",
    "clean:mkdirp": "mkdirp dist/js lib lib/pegjs",
    "flow": "flow",
    "lint": "eslint --color src scripts webpack.config.babel.js",
    "test": "jest --color",
    "travis-ci": "run-s lint test",
    "watch": "run-p -l watch:*:* watch:server",
    "watch:build:fluorite5": "nodemon -d 1 -w fluorite5 -e pegjs -x \"npm run build:fluorite5\"",
    "watch:build:webpack": "npm run build:webpack -- -w",
    "watch:lint": "npm run lint -- -w",
    "watch:test": "npm run test -- -w",
    "watch:server": "nodemon -d 1 -w src --ignore src/browser --ignore **/__tests --ignore **/__mocks__ --exec \"babel-node src/server\"",
    "production": "run-s -l clean lint test build:* prod:*",
    "prod:webpack": "cross-env NODE_ENV=production webpack",
    "version": "update-changelog && git add CHANGELOG.md"
  },
  "author": "ukatama",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "bcdice-js": "^0.6.0",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "color-space": "^1.13.0",
    "config": "^1.26.2",
    "connect-livereload": "^0.6.0",
    "connect-redis": "^3.0.2",
    "connect-session-knex": "^1.0.17",
    "dice3d": "^1.0.2",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "express-socket.io-session": "^1.3.1",
    "format-si-prefix": "^0.3.2",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "is-mobile": "^0.2.2",
    "knex": "^0.13.0",
    "lodash": "^4.17.19",
    "log4js": "^2.3.3",
    "mariasql": "^0.2.6",
    "material-ui": "^0.19.0",
    "moment": ">=2.11.2",
    "mysql": "^2.14.1",
    "mysql2": "^1.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pegjs": "^0.10.0",
    "pq": "^0.0.3",
    "pug": "^2.0.0-rc.3",
    "react": "^15.1.0",
    "react-addons-perf": "^15.1.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-immutable-proptypes": "^2.0.0",
    "react-redux": "^5.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "redis": "^2.5.1",
    "redux": "^3.0.5",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.15.6",
    "redux-tower": "^0.0.6",
    "sanitize.css": "^5.0.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "source-map-support": "^0.4.0",
    "sqlite3": "^3.1.9",
    "update-changelog": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.54.1",
    "jasmine-immutablejs-matchers": "^0.1.2",
    "jest": "^21.0.2",
    "make-dir-cli": "^1.0.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.2",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.5.3",
    "webpack": "^3.5.5"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov",
      "clover"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jest-setup.js",
    "automock": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/ansi-regex",
      "<rootDir>/node_modules/any-promise",
      "<rootDir>/node_modules/bluebird",
      "<rootDir>/node_modules/body-parser",
      "<rootDir>/node_modules/bookshelf/node_modules/bluebird",
      "<rootDir>/node_modules/bookshelf/node_modules/lodash",
      "<rootDir>/node_modules/depd",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/knex/node_modules/bluebird",
      "<rootDir>/node_modules/knex/node_modules/lodash",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/material-ui",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/recompose",
      "<rootDir>/node_modules/sshpk"
    ]
  }
}
